UserParameter=nginx.status[*],(/etc/zabbix/ngx_status.sh $1)
UserParameter=netstat.stat[*],(netstat -ant |grep -i $1|wc -l)
UserParameter=access.status[*],(/etc/zabbix/access_status.sh $1 $2)
UserParameter=nginx.upstream[*],(curl -s -o /dev/null -A "Chrome"-I -w "%{http_code}" "$1")
UserParameter=nginx.pingupstream[*],(ping -c 10 -w 2 "$1" | grep 'loss' | awk -F',' '{ print $$3 }' | awk -F"%" '{ print $$1 }')
